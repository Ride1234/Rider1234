#include <iostream>
#include <stdio.h>
using namespace std;
int main()
{
    int x,y;
    printf("please input x: \n");
    scanf("%d",&x);
    
    printf("please input y: \n");
    scanf("%d",&y);
    /*
    + - * / %
    
    ++x
    x++
    --x
    x--
    
    x = 16
    x >> 1
    x >> 2
    x << 1
    
    */
    
    printf(" %d + %d = %d \n ",x,y,x+y);
    
    cout<<"Hello World";

    return 0;
}
